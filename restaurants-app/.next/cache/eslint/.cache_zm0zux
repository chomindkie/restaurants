rants, handlerTarget}) {\n\n    // eslint-disable-next-line react-hooks/rules-of-hooks\n    const onClick = (restaurant) => useCallback(function () {\n        handlerTarget(restaurant.placeId)\n    }, []);\n\n    return (\n        <main className={styles.section}>\n            <section className={styles.container}>\n                <div className={styles.layout}>\n                    {restaurants.map((restaurant) => (\n                        <Card\n                            key={restaurant.placeId}\n                            title={restaurant.name}\n                            rating={restaurant.rating}\n                            image={restaurant.image}\n                            onClick={onClick(restaurant)}\n                        />\n                    ))}\n                </div>\n            </section>\n        </main>\n    );\n}\n\nexport default RestaurantList;\n","/Users/chomind/Documents/workSpce/scg/restaurants/restaurants-app/components/SearchBar.js",[],[],{"ruleId":"66","severity":1,"message":"67","line":32,"column":8,"nodeType":"68","endLine":32,"endColumn":10,"suggestions":"69"},{"ruleId":"70","severity":1,"message":"71","line":22,"column":21,"nodeType":"72","endLine":22,"endColumn":101},{"ruleId":"70","severity":1,"message":"71","line":6,"column":17,"nodeType":"72","endLine":6,"endColumn":51},{"ruleId":"73","severity":1,"message":"74","line":6,"column":17,"nodeType":"72","endLine":6,"endColumn":51},{"ruleId":"70","severity":1,"message":"71","line":9,"column":17,"nodeType":"72","endLine":9,"endColumn":47},{"ruleId":"73","severity":1,"message":"74","line":9,"column":17,"nodeType":"72","endLine":9,"endColumn":47},{"ruleId":"70","severity":1,"message":"71","line":12,"column":17,"nodeType":"72","endLine":12,"endColumn":47},{"ruleId":"73","severity":1,"message":"74","line":12,"column":17,"nodeType":"72","endLine":12,"endColumn":47},{"ruleId":"66","severity":1,"message":"75","line":13,"column":8,"nodeType":"68","endLine":13,"endColumn":10,"suggest